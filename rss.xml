---
---
<?xml version="1.0" encoding="utf-8"?>
<feed 
  xmlns="http://www.w3.org/2005/Atom"
  xmlns:thr="http://purl.org/syndication/thread/1.0"
  xml:lang="en-US"
   >
	<title type="text">{{site.name}}</title>
	<subtitle type="text" >{{site.description}}</subtitle>
	
	<updated>{{site.time | date_to_xmlschema}}</updated>

	<link href="http://www.behavaic.com/" rel="alternate" type="text/html" />
	<id>{{site.url}}{{site.baseurl}}/</id>
	<link rel="self" type="application/atom+xml" href="{{site.url}}{{site.baseurl}}/rss.xml" />

	<generator uri="{{site.url}}" version="1.0">behavaic Team</generator>
	{% for post in site.posts %}
	<entry>
		<title type="html"><![CDATA[{{post.title}}]]></title>
		<link href="{{site.url}}{{site.baseurl}}{{post.url | uri_escape }}" rel="alternate" type="text/html" title="{{post.title | xml_escape}}" />
		<id>{{site.url}}{{site.baseurl}}{{post.url | uri_escape }}</id>
		<published>{{post.date | date_to_xmlschema}}</published>
		<updated>{{post.date | date_to_xmlschema }}</updated>
		<content type="html"  xml:base="{{site.url}}{{site.baseurl}}{{post.url | uri_escape }}"><![CDATA[{{post.content}}]]></content>
		<author>
			<name>{{post.author}}</name>
			<uri>https://github.com/{{post.author}}</uri>
		</author>
		
	</entry>
	{% endfor %}

	{% for post in site.docs %}
	<entry>
		<title type="html"><![CDATA[{{post.title}}]]></title>
		<link href="{{site.url}}{{site.baseurl}}{{post.url | uri_escape }}" rel="alternate" type="text/html" title="{{post.title | xml_escape}}" />
		<id>{{site.url}}{{site.baseurl}}{{post.url | uri_escape }}</id>
		<published>{{post.date | date_to_xmlschema}}</published>
		<updated>{{post.date | date_to_xmlschema }}</updated>
		<content type="html"  xml:base="{{site.url}}{{site.baseurl}}{{post.url | uri_escape }}"><![CDATA[{{post.content}}]]></content>
		<author>
			<name>{{post.author}}</name>
			<uri>https://github.com/{{post.author}}</uri>
		</author>
		
	</entry>
	{% endfor %}

</feed>
